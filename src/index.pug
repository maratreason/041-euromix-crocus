extends pug/base.pug

append content
  include pug/includes/header.pug

  main.main(id="main")
    section.section-hero
      img.section-hero__image(src="images/main/main-bg.jpg" alt="")
      .section-hero__block
        .container
          .section-hero__title Мобильный бетонный завод
            span.orange EUROMIX CROCUS

          form.section-hero__form.form
            .form__title Оставьте заявку
            .form__subtitle Мы отправим Вам цены, условия и сроки поставки
            .form__inputs
              input.input(type="text" placeholder="Ваше имя")
              input.input(type="text" placeholder="Телефон")
            .form__btn
              button.btn.btn-sm.btn-black(type="submit") Отправить

            .form__checkbox.checkbox
              input.checkbox__input(type="checkbox" id="checkbox-1" checked)
              label.checkbox__label(for="checkbox-1") согласен с пользовательским<br> соглашением

